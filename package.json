{
  "name": "parcel-starter",
  "version": "1.1.0",
  "description": "Front-end starter kit using the Parcel bundler",
  "main": "index.js",
  "author": "Ajith R N <dev@ajithrn.com>",
  "dependencies": {},
  "devDependencies": {
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "parcel": "latest",
    "parcel-plugin-custom-dist-structure": "^1.1.14",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "popper.js": "^1.16.0",
    "sass": "^1.15.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel src/index.html --open",
    "build": "parcel build src/index.html --no-source-maps --public-url ./"
  },
  "customDistStructure": {
    "config": {
      ".js": "assets/js",
      ".css": "assets/styles",
      "assets/images": [
        ".jpg",
        ".png",
        ".svg"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajithrn/parcel-starter-kit.git"
  },
